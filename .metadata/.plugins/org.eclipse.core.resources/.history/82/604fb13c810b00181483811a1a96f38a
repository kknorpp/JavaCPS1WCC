package alphabet;

public class alphabetVowels {

	public static void main(String[] args) {

		char c;
		for (c = 'a'; c <= 'z'; ++c) {
			while (c >= 'a' && c <= 'z') {
				switch (c) {
				case 'a':
					System.out.println("a is a vowel.");
				case 'b':
					System.out.println("b is a consonant.");
				case 'c':
					System.out.println("c is a consonanat.");
				case 'd':
					System.out.println("d is a consonant.");
				case 'e':
					System.out.println("e is a vowel.");
				case 'f':
					System.out.println("f is a consonant.");
				case 'g':
					System.out.println("g is a consonant.");
				case 'h':
					System.out.println("h is a consonant.");
				case 'i':
					System.out.println("i is a vowel.");
				case 'j':
					System.out.println("j is a consonant.");
				case 'k':
					System.out.println("k is a consonant.");
				case 'l':
					System.out.println("l is a consonant.");

				case 'm':
					System.out.println("m is a consonant.");
				case 'n':
					System.out.println("n is a consonant.");
				case 'o':
					System.out.println("o is a vowel.");
				case 'p':
					System.out.println("p is a consonant.");
				case 'q':
					System.out.println("q is a consonant.");
				case 'r':
					System.out.println("r is a consonant.");
				case 's':
					System.out.println("s is a consonant.");
				case 't':
					System.out.println("t is a consonant.");
				case 'u':
					System.out.println("u is a vowel.");

				case 'v':
					System.out.println("v is a consonant.");

				case 'w':
					System.out.println("w is a consonant.");
				case 'x':
					System.out.println("x is a consonant.");
				case 'y':
					System.out.println("y is a vowel.");

				case 'z':
					System.out.println("z is a consonant.");
					break;
				default:
					System.out.println();
				}
			}
		}
	}

}
