package classWork5;

public class letterCombinationNestedLoop {
	public static void main(String[] args) {
		int i;
		char letter;
		char letter1;
		char letter2, letter3;
		for (letter = 'a'; letter <= 'z'; letter++) {

			for (letter1 = 'a', letter1 = 'i';  letter1 <= 'z'; letter1++, i++) {

					System.out.print(" " + letter + letter1 + " ");

					System.out.print(" " + letter + letter1 + " ");
				}

			}
		}
	}

}
