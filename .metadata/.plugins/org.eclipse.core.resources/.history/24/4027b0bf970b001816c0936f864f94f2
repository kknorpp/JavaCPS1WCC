package classWork5;

public class letterCombinationNestedLoop {
	public static void main(String[] args) {

		char letter;
		char letter1;
		for (letter = 'a'; letter <= 'z'; letter++) {
			
			for (letter1 = 'a'; letter1 <= 'z'; letter1++) {
				System.out.println(" " + letter + letter1);
			}

		}
	}
}
