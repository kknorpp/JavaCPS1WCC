package alphabet;

public class alphabetVowels {

	public static void main(String[] args) {

		char c;

			for (c = 'a'; c <= 'z'; c++) {
				while (c <= 'z') {

				
						switch (c) {
						case 'a':
							return ("a is a vowel.");
						case 'b':
							return ("b is a consonant.");
						case 'c':
							return ("c is a consonanat.");
						case 'd':
							return ("d is a consonant.");
						case 'e':
							return ("e is a vowel.");
						case 'f':
							return ("f is a consonant.");
						case 'g':
							return ("g is a consonant.");
						case 'h':
							return ("h is a consonant.");
						case 'i':
							return ("i is a vowel.");
						case 'j':
							return ("j is a consonant.");
						case 'k':
							return ("k is a consonant.");
						case 'l':
							return ("l is a consonant.");

						case 'm':
							return ("m is a consonant.");
						case 'n':
							return ("n is a consonant.");
						case 'o':
							return ("o is a vowel.");
						case 'p':
							return ("p is a consonant.");
						case 'q':
							return ("q is a consonant.");
						case 'r':
							return ("r is a consonant.");
						case 's':
							return ("s is a consonant.");
						case 't':
							return ("t is a consonant.");
						case 'u':
							return ("u is a vowel.");

						case 'v':
							return ("v is a consonant.");

						case 'w':
							return ("w is a consonant.");
						case 'x':
							return ("x is a consonant.");
						case 'y':
							return ("y is a vowel.");

						case 'z':
							return ("z is a consonant.");
c++;
						}System.exit(0);

					}
				}

}

}
